
public record Benificiary(String name, String Accno, String bankName) {
	public Benificiary{
		if(name.isBlank()|| name==null) {
			System.out.println("Null name");
		}
		if(Accno.isBlank()|| Accno==null) {
			System.out.println("Null Accno");
		}
		if(bankName.isBlank()|| bankName==null) {
			System.out.println("Null bankName");
		}
	}
	
	public Benificiary updatedbankName(String newbankName) {
		return new Benificiary(name,Accno,newbankName);
	}
	
	public Benificiary updatename(String newName) {
		return new Benificiary(newName, Accno, bankName);
	}
	
	public Benificiary updateAccno(String newAccno) {
		return new Benificiary(name,newAccno, bankName);
	}
	
	public Benificiary updateDetails(String newName, String newAccno, String newbankName) {
		return new Benificiary(newName,newAccno,newbankName);
	}
	
//	public void display() {
//		Benificiary.forEach(System.out::println);
//	}
	
}

