import java.util.*;

class BadKey {
    private String name;

    public BadKey(String name) {
        this.name = name;
    }

    @Override
    public int hashCode() {
        // Force all keys to have the same hash value
        return 100; 
    }

    @Override
    public boolean equals(Object obj) {
        if (this == obj) return true;
        if (obj == null || getClass() != obj.getClass()) return false;
        BadKey other = (BadKey) obj;
        return Objects.equals(name, other.name);
    }

    @Override
    public String toString() {
        return name;
    }
}

public class HashCode {
    public static void main(String[] args) {
        Map<BadKey, String> map = new HashMap<>();

        BadKey k1 = new BadKey("Alice");
        BadKey k2 = new BadKey("Bob");

        // both will have same hashcode, different keys
        map.put(k1, "Engineer");
        map.put(k2, "Manager");

        // Demonstrate internal behavior
        System.out.println("Hash of k1: " + k1.hashCode());
        System.out.println("Hash of k2: " + k2.hashCode());
        System.out.println("Are keys equal? " + k1.equals(k2));

        // Show map contents
        map.forEach((k, v) -> System.out.println(k + " -> " + v));

        // Retrieve values
        System.out.println("Get Alice: " + map.get(k1));
        System.out.println("Get Bob: " + map.get(k2));
    }
}
