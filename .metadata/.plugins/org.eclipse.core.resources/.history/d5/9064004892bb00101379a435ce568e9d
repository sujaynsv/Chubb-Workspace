import java.util.*;
public class HashCode {
	public static void main(String[] args) {
		Map<String, Integer> mpp=new HashMap<>();
		mpp.put("Hey", 32);
		mpp.put("Hi", 12);
		System.out.println("Hashchode of Hey: "+"Hey".hashCode());
		System.out.println("Haschode of Hi: "+"Hi".hashCode());
		System.out.println("Checking with equals: "+"Hey".equals("Hi"));
		
		
		
	}
}
